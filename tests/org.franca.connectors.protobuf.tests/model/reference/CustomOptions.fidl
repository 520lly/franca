	}

	struct ServiceOptions_6 {
		MyEnum my_service_option
	}

	struct MethodOptions_7 {
		MyMessage my_method_option
	}

	struct MyMessage {
		Int32 foo
		String bar
	}

	enumeration MyEnum {
		FOO = 1
		BAR = 2
	}

	struct RequestType polymorphic {
	}

	struct ResponseType polymorphic {
	}

}