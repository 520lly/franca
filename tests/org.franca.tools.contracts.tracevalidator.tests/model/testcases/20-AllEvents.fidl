
package testcases

interface Interface10 {

	attribute UInt16 a
	
	method m { }

	broadcast b { }
	
	contract {
		PSM {
			initial reading
			state reading {
				on call m -> waitingForWriting
				on update a -> reading
			}
			state waitingForWriting {
				on respond m -> writing
			}
			state writing {
				on signal b -> reading
				on set a -> writing
			}
		}
	}
}
