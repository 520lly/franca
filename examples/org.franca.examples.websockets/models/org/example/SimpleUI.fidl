package org.example

/*
 * Interface used for HTML5/Websockets example.
 */
interface SimpleUI {

	attribute String title

	method setMode fireAndForget {
		in  { String p1 }
		//out { String p2 }
	}
	
	broadcast updateVelocity {
		out { UInt16 velocity }
	}
	
	enumeration Mode {
		M_RADIO
		M_NAVIGATION
		M_MULTIMEDIA
		M_SETTINGS
	}
}

